{
  "name": "react-tablenex",
  "version": "1.4.0",
  "description": "A modern, customizable React table component built with TypeScript, featuring responsive design, fixed columns, and expandable rows.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "table",
    "typescript",
    "component",
    "responsive",
    "data-table",
    "fixed-columns",
    "expandable-rows",
    "ui",
    "react-component",
    "customizable"
  ],
  "author": "Devyansh Yadav <devyanshdeveloper@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devyanshyadav/react-tablenex.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0 || ^19.0.0-rc",
    "react-dom": "^18.0.0 || ^19.0.0 || ^19.0.0-rc"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
